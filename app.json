{
  "expo": {
    "name": "CommUnity",
    "slug": "community",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/community_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["comgooglemaps", "comgooglemaps"]
      },
      "bundleIdentifier": "com.anonymous.community"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/community_logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.anonymous.community"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/community_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/community_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to share images.",
          "cameraPermission": "The app needs access to your camera to take photos."
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b85239e2-76f5-4f66-9ad9-7fe596bf1aaf"
      }
    },
    "owner": "communityuo"
  }
}
